# MongoDB connection string provided by Railway
# This will be automatically set by Railway when you connect your MongoDB service
MONGODB_URI=mongodb://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# MongoDB database name
MONGODB_DATABASE=collaborative_editor

# Port for the WebSocket server
# Railway will assign its own port, so usually you don't need to set this
PORT=8887

# Railway environment indicator
RAILWAY_ENVIRONMENT=production 